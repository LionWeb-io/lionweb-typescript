{
  "name": "@lionweb/cli",
  "version": "0.6.14-beta.0",
  "description": "LionWeb CLI for {Java|Type}Script",
  "bin": {
    "lionweb-cli": "./dist/lionweb-cli.js"
  },
  "main": "dist/lionweb-cli.js",
  "type": "module",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LionWeb-io/lionweb-typescript.git"
  },
  "bugs": {
    "url": "https://github.com/LionWeb-io/lionweb-typescript/issues"
  },
  "dependencies": {
    "@lionweb/core": "0.6.13-beta.0",
    "@lionweb/utilities": "0.6.13-beta.0",
    "@lionweb/validation": "0.6.13-beta.0"
  },
  "scripts": {
    "clean": "rm -rf dist/ lionweb-cli-*.tgz",
    "build": "tsc",
    "lint": "eslint src",
    "test": "node dist/lionweb-cli.js",
    "pre-release-either": "npm run clean && npm install && npm run build",
    "prerelease": "npm run pre-release-either",
    "release": "npm publish",
    "prerelease-beta": "npm run pre-release-either",
    "release-beta": "npm publish --tag beta"
  }
}
