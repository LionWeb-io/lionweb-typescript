        "lint": "eslint src"
    },
    "dependencies": {
        "@lionweb/core": "0.7.0-beta.15",
        "@lionweb/class-core": "0.7.0-beta.15",
        "@lionweb/class-core-generator": "0.7.0-beta.15",
        "@lionweb/json": "0.7.0-beta.15",
        "@lionweb/textgen-utils": "0.7.0-beta.15",
        "@lionweb/ts-utils": "0.7.0-beta.15",
        "@lionweb/utilities": "0.7.0-beta.15",
        "littoral-templates": "0.5.1"
    },
    "devDependencies": {
        "@types/node": "24.3.0"
    }
