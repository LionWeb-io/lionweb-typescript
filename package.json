{
    "name": "lionweb-typescript",
    "version": "0.7.0-dev",
    "description": "LionWeb implementation for {Java|Type}Script",
    "license": "Apache-2.0",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/LionWeb-io/lionweb-typescript.git"
    },
    "bugs": {
        "url": "https://github.com/LionWeb-io/lionweb-typescript/issues"
    },
    "devDependencies": {
        "@eslint/eslintrc": "3.3.3",
        "@eslint/js": "9.39.2",
        "@types/node": "25.2.2",
        "@typescript-eslint/eslint-plugin": "8.54.0",
        "@typescript-eslint/parser": "8.54.0",
        "dpdm": "4.0.1",
        "globals": "17.3.0",
        "prettier": "3.8.1",
        "rimraf": "6.1.2",
        "typescript": "5.9.3"
    },
    "scripts": {
        "clean": "npm run clean -w packages --if-present && npx rimraf node_modules",
        "setup": "npm run setup -w packages --if-present",
        "initialize": "npm run clean && npm install && npm run setup",
        "rebuild": "sh rebuild.sh",
        "update-version": "node update-package-versions.js",
        "build": "npm run build -w packages --if-present",
        "test": "npm run test -w packages --if-present",
        "lint": "npm run lint -w packages --if-present",
        "prerelease": "npm run initialize",
        "release-alpha": "npm run release-alpha -w packages --if-present",
        "release-beta": "npm run release-beta -w packages --if-present",
        "release": "npm run release -w packages --if-present",
        "check-circular-deps": "node check-circular-dependencies.js",
        "publish-local": "npm run publish-local --workspaces --if-present",
        "unpublish-local": "npm run unpublish-local --workspaces --if-present",
        "republish-local": "npm run unpublish-local && npm run publish-local"
    },
    "private": true,
    "workspaces": [
        "./packages/ts-utils",
        "./packages/textgen-utils",
        "./packages/json",
        "./packages/json-utils",
        "./packages/json-diff",
        "./packages/core",
        "./packages/node-utils",
        "./packages/utilities",
        "./packages/validation",
        "./packages/cli",
        "./packages/io-lionweb-mps-specific",
        "./packages/class-core-generator",
        "./packages/class-core",
        "./packages/build",
        "./packages/class-core-test-language",
        "./packages/class-core-test",
        "./packages/test",
        "./packages/delta-protocol-common",
        "./packages/delta-protocol-client",
        "./packages/delta-protocol-low-level-client-ws",
        "./packages/delta-protocol-low-level-client-browser",
        "./packages/delta-protocol-repository-ws",
        "./packages/delta-protocol-test-cli",
        "./packages/delta-protocol-test"
    ]
}
